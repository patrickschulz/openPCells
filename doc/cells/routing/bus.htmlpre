<!doctype html>
<html lang="en">
    @make_header OpenPCells Documentation
    <body>
        @make_topbar Cell Documentation &ndash; routing/bus
        <p>
        This is the the documentation of the <code class="generic">routing/bus</code> cell.
        </p>

        <details open>
            <summary>Cell Info</summary>
        </details>

        <details open>
            <summary>Cell Parameters</summary>
            <table class="parameters">
                <colgroup>
                    <col class="name"/>
                    <col class="type"/>
                    <col class="info"/>
                    <col class="default"/>
                    <col class="allowed"/>
                </colgroup>
                <tr>
                    <th>Name</th>
                    <th>Type</th>
                    <th>Info</th>
                    <th>Default Value</th>
                    <th>Allowed Values</th>
                </tr>
                <tr>
                    <td>numbits</td>
                    <td>integer</td>
                    <td>Number of bits per register and merge</td>
                    <td>8</td>
                    <td>Any</td>
                </tr>
            </table>
        </details>

        <details open>
            <summary>Cell Anchors</summary>
        </details>

        @include footer.html
    </body>
</html>
